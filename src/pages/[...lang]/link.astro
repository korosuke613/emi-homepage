---
import { defaultLang, languageKeys } from "../../i18n/ui";
import DefaultLink from "../link.astro";

export async function getStaticPaths() {
  return languageKeys
    .map((lang) => {
      if (lang === defaultLang) {
        return undefined;
      }
      return { params: { lang } };
    })
    .filter((v) => v !== undefined);
}

const { lang } = Astro.params;
---

<DefaultLink lang={lang}></DefaultLink>
