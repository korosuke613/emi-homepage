    "@emotion/styled": "11.13.0",
    "@fontsource/noto-serif-jp": "5.2.5",
    "@mui/icons-material": "6.0.2",
    "@mui/joy": "5.0.0-beta.48",
    "@mui/material": "6.0.2",
    "@types/react": "18.3.8",
    "@types/react-dom": "18.3.0",
    "@vercel/speed-insights": "1.0.12",
    "astro": "5.9.3",
