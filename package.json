{
  "name": "tremendous-asteroid",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "check": "astro check",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "production-dev": "PRODUCTION=true astro dev",
    "production-build": "PRODUCTION=true astro build",
    "production-preview": "PRODUCTION=true astro preview",
    "astro": "astro",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
